{
  "name": "term-structure-core",
  "version": "0.0.1",
  "author": "TKSpring",
  "license": "",
  "scripts": {
    "reset:db": "npm run schema:drop && npm run typeorm:run-migrations && npm run seed:run",
    "postinstall": "patch-package",
    "pm2": "pm2 delete all && pm2 start",
    "build": "nest build ts-core",
    "build:api": "nest build ts-rollup-api",
    "start": "node dist/ts-core/main.js",
    "start:api": "node dist/ts-rollup-api/main.js",
    "dev": "nest start ts-core --debug --watch",
    "dev:api": "nest start ts-rollup-api --debug --watch",
    "dev:all": "nest start ts-core ts-rollup-api --debug --watch",
    "test:unit-r": "jest --config ./jest-unit.json",
    "test:unit": "jest --config ./jest-unit.json --coverage",
    "test:inte": "jest --config ./jest-integration.json --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json --runInBand --forceExit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "eslint": "eslint --ext .ts ./ --fix",
    "abi2ts:verifier": "abi-types-generator './ts-operator/src/domain/verified-abi.json' --output='./ts-operator/src/domain' --name=verifier-contract",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "schema:sync": "yarn run typeorm schema:sync -d common/ts-typeorm/src/typeOrm.config.ts",
    "schema:drop": "yarn run typeorm schema:drop -d common/ts-typeorm/src/typeOrm.config.ts",
    "schema:log": "yarn run typeorm schema:log -d common/ts-typeorm/src/typeOrm.config.ts",
    "typeorm:show": "yarn run typeorm migration:show -d common/ts-typeorm/src/typeOrm.config.ts", 
    "typeorm:run-migrations": "yarn run typeorm migration:run -d common/ts-typeorm/src/typeOrm.config.ts",
    "typeorm:create-migration": "npm run typeorm -- migration:create common/ts-typeorm/src/migrations/$npm_config_name",
    "typeorm:generate-migration": "npm run typeorm -- migration:generate -d common/ts-typeorm/src/typeOrm.config.ts common/ts-typeorm/src/migrations/$npm_config_name",
    "typeorm:revert-migration": "yarn run typeorm migration:revert -d common/ts-typeorm/src/typeOrm.config.ts",
    "typeorm:gen": "source .env && npx typeorm-model-generator --host $DB_HOST --database $DB_NAME --user $DB_USER --pass tkspwd --port 5432 --engine postgres --ssl false",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "NODE_OPTIONS=--max_old_space_size=102400 ts-node ./node_modules/typeorm-seeding/dist/cli.js seed"
  },
  "dependencies": {
    "@anchan828/nest-bullmq": "^3.1.4",
    "@anchan828/nest-bullmq-terminus": "^3.1.4",
    "@automapper/core": "^8.7.7",
    "@automapper/nestjs": "^8.7.7",
    "@big-whale-labs/poseidon": "^0.0.5",
    "@imqueue/pg-pubsub": "^1.6.0",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/cqrs": "^8.0.5",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/terminus": "^9.1.3",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.2.1",
    "@reactivex/rxjs": "^6.6.7",
    "@types/bcrypt": "^5.0.0",
    "assert": "^2.0.0",
    "axios": "^1.2.0",
    "bcrypt": "^5.0.1",
    "bignumber.js": "^9.1.0",
    "blake-hash": "^2.0.0",
    "bullmq": "^3.2.2",
    "circomlibjs": "^0.1.7",
    "commitizen": "^4.2.4",
    "ethers": "^5.7.1",
    "express-ctx": "^0.1.1",
    "ffjavascript": "^0.2.56",
    "fp-ts": "^2.10.5",
    "io-ts": "^2.2.16",
    "io-ts-types": "^0.5.16",
    "lodash": "^4.17.21",
    "merkletreejs": "^0.2.32",
    "nestjs-ethers": "^1.0.3",
    "nestjs-pino": "^1.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "runtypes": "^6.3.1",
    "rxjs": "^7.5.7",
    "snarkjs": "^0.5.0",
    "source-map-support": "^0.5.19",
    "swagger-ui-express": "^4.1.6",
    "ts-loader": "^9.2.3",
    "typeorm": "^0.3.11",
    "typescript": "^4.8.4",
    "util": "^0.12.5",
    "util.promisify": "^1.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^3.15.2",
    "@golevelup/ts-jest": "^0.3.4",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^8.0.6",
    "@nestjs/testing": "^8.0.6",
    "@types/cron": "^2.0.0",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.9",
    "@types/passport-jwt": "^3.0.6",
    "@types/source-map-support": "^0.5.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "abi-types-generator": "^1.1.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.27.0",
    "ethereum-abi-types-generator": "^1.3.2",
    "jest": "^29.3.1",
    "leaked-handles": "^5.2.0",
    "patch-package": "^6.5.0",
    "prettier": "^2.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typeorm-seeding": "^1.6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
